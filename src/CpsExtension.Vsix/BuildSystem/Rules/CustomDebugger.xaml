<Rule
    Name="CustomDebugger"
    DisplayName="Custom Debugger"
    PageTemplate="debugger"
    Description="Custom Debugger options"
    xmlns="http://schemas.microsoft.com/build/2009/properties">
    <Rule.DataSource>
        <DataSource Persistence="UserFileWithXamlDefaults" HasConfigurationCondition="True"/>
    </Rule.DataSource>

    <StringProperty Name="CustomProperty" DisplayName="Custom Property"
                Default="Default Value"
                Description="The debug custom property." />
</Rule>